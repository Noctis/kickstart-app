{
    "name": "noctis/kickstart-app",
    "description": "The user application part of the Kickstart project.",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../kickstart"
        }
    ],
    "require": {
        "php": "^8.0",
        "noctis/kickstart": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
