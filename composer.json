{
    "name": "noctis/kickstart-app",
    "description": "The user application part of the Kickstart project.",
    "keywords": ["kickstart", "skeleton app", "base application"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Noctis/kickstart.git"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "noctis/kickstart": "^2.0",
        "paragonie/easydb": "^2.11"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env') || copy('.env-example', '.env');\""
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
